<h2>Project <%= @project.name %></h2>

<ul>
	<% @entries.each do |entry| %>
		<li> 
		<%=entry.hours%> h, 
		<%=entry.minutes%> m - 
		<%=entry.date%> 
		<%= link_to "Edit", "/projects/#{@project.id}/time_entries/#{entry.id}/edit" %>
		</li>
	<% end %>
</ul>


<br>
